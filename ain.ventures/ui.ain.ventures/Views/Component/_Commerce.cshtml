@model SourceProduct

@{ 
    var divId = string.Concat("SourceProduct", Model.ItemKey);
}
<div data-mouseover='showEditIcons'
     data-mouseout='hideEditIcons'
     data-action='buyProduct' 
     data-params='{
        "url":"@Model.Url", 
        @*"user":"@(((User)Session["User"]).GUID)",*@ 
        "ItemKey": "@Model.ItemKey", 
        "ParentGUID":"@Model.ParentGUID",
        "DivID":"@divId" 
     }' 
     id='divID'
     class='SourceProductIcon'
     >
    <img class='ComponentCommerceIcon' src='~/Images/commerce.svg' />
    <div data-action='editProduct' 
         data-params='new {"ParentIcon":"@string.Concat("SearchIcon", Model.ComponentGUID)" }'
         id='@String.Concat("BuyProductIcon", Model.ItemKey)'
         class='Edit'>
        <svg width='15' height='15'>
            <line x1='10' y1='5' x2='13' y2='2' style='stroke:black;stroke-width:3'></line>
            <line x1='5' y1='9' x2='9' y2='5' style='stroke:black;stroke-width:3'></line>
            <polygon points='4,8 6,10 2,13' style='fill:black'></polygon>
        </svg>
    </div>
    <div data-action='deleteProduct'
         data-params='new {"ParentIcon":"@string.Concat("SearchIcon", Model.ComponentGUID)" }'
         id='@String.Concat("BuyProductIcon", Model.ItemKey)'
         class='Delete'>
        <svg width='15' height='15'>
            <line x1='3' y1='3' x2='12' y2='12' style='stroke:rgb(0,0,0);stroke-width:2'></line>
            <line x1='3' y1='12' x2='12' y2='3' style='stroke:rgb(0,0,0);stroke-width:2'></line>
        </svg>
    </div>
</div>